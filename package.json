{
  "name": "take-home",
  "version": "1.0.0",
  "description": "Take home exercise for SPA",
  "main": "webpack.config.js",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "knex": "^0.16.3",
    "sqlite3": "^4.0.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/uuid": "^3.4.4",
    "nodemon": "^1.18.10",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  },
  "scripts": {
    "start:server": "cd ./src/server && ts-node ./index.ts",
    "watch:server": "webpack --display=minimal --watch --config ./webpack.server.config.js",
    "migrate:up": "./node_modules/.bin/knex migrate:latest --knexfile ./src/server/knexfile.ts",
    "migrate:down": "./node_modules/.bin/knex migrate:rollback --knexfile ./src/server/knexfile.ts",
    "seed": "NODE_ENV=seed ./node_modules/.bin/knex seed:run --knexfile ./src/server/knexfile.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/100health/take-home.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/100health/take-home/issues"
  },
  "homepage": "https://github.com/100health/take-home#readme"
}
