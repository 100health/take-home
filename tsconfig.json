{
  "compilerOptions": {
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "lib": ["dom", "es2015"],
    "module": "commonjs",
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "noImplicitAny": true
  },
  "include": ["./src/**/*"]
}
